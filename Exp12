#include<stdio.h>
int main()
{
	int n,temp,result=0,rem;
	printf("enter the number:");
	scanf("%d",&n);
	temp=n;
	while(n!=0)
	{
		rem=n%10;
		result=result+(rem*rem*rem);
		n=n/10;
	}
	if(result==temp)
	{
		printf("%d is amstrong number.",temp);
	}
	else
	{
		printf("%d is not amstorng number.",temp);
	}
	return 0;
}
